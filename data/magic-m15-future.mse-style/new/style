	type: choice
	name: future node
	default: typesymbol_type(type: card.super_type)
	card list visible: false
	show statistics: false
	choice: artifact
	choice: creature
	choice: enchantment
	choice: fortress
	choice: instant
	choice: land
	choice: multitype
	choice: planeswalker
	choice: sorcery
	choice: aetherprint
	choice: none
	description: The card type symbol for this card
