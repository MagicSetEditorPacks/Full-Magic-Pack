	"Italiano": [
		code              : "it"
		spellcheck_code   : "it_IT"
		fallback          : "English"
		pt_separator      : "/"
		type_separator    : " — "
		super_spacer      : true
		subtype_separator : " "
		keyword_types     : ["vecchio","nucleo","esperto","pseudo","azione","personalizzato"]

		is_artifact       : match@(match: "(?i)Artefatto")
		is_battle         : match@(match: "(?i)Battaglia")
		is_creature       : match@(match: "(?i)Creatura")
		is_kindred        : match@(match: "(?i)(Fazione|Tribale)")
		is_tribal         : match@(match: "(?i)(Fazione|Tribale)")
		is_enchantment    : match@(match: "(?i)Incantesimo")
		is_land           : match@(match: "(?i)Terra")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Istantaneo|Stregoneria")
		is_instant        : match@(match: "(?i)Istantaneo")
		is_sorcery        : match@(match: "(?i)Stregoneria")

		is_nonstandard    : match@(match: "(?i)(Cospirazione|Dungeon|Emblema|Eroe|Fenomeno|Piano|Schema|Pedina|Avanguardia)")
		is_conspiracy     : match@(match: "(?i)Cospirazione")
		is_dungeon        : match@(match: "(?i)Dungeon")
		is_emblem         : match@(match: "(?i)Emblema")
		is_hero           : match@(match: "(?i)Eroe")
		is_token          : match@(match: "(?i)Pedina")
		is_phenomenon     : match@(match: "(?i)Fenomeno")
		is_plane		  : match@(match: "(?i)Piano")
		is_scheme         : match@(match: "(?i)Schema")
		is_vanguard       : match@(match: "(?i)Avanguardia")

		is_basic          : match@(match: "(?i)Base")
		is_elite          : match@(match: "(?i)Elite")
		is_host           : match@(match: "(?i)Ospite")
		is_legendary      : match@(match: "(?i)Leggendari[ao]")
		is_ongoing        : match@(match: "(?i)in Corso")
		is_snow           : match@(match: "(?i)Neve")
		is_world          : match@(match: "(?i)del mondo")
		get_supertypes    : filter_text@(match: "(?i)(Leggendari[ao]|Base|Neve|Pedina|del mondo|in Corso|Elite|Ospite) ?")
		remove_supertypes : replace_text@(match: "(?i)(Leggendari[ao]|Base|Neve|Pedina|del mondo|in Corso|Elite|Ospite)[- ]?", replace:"")
		base_supertypes   : "Leggendari[ao]|Base|Neve|Pedina|del mondo|in Corso|Elite|Ospite"

		is_plains         : match@(match: "(?i)Pianura")
		is_island         : match@(match: "(?i)Isola")
		is_swamp          : match@(match: "(?i)Palude")
		is_mountain       : match@(match: "(?i)Montagna")
		is_forest         : match@(match: "(?i)Foresta")
		is_wastes         : match@(match: "(?i)Distesa")

