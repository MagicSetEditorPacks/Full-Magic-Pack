
############################################################## Statistics dimensions 30-03-2023
############################################################## Blame GenevensiS for this

statistics dimension:
	name: color category
	position hint: 0000
	icon: stats/color.png
	description: How many cards of each frame color are in the set. Only looking at the front face colors.
	split list: true
	show empty: false
	script: if filter_from_statistic() == "Filtrado" or trim_from_statistic() then "" else color_category_statistic()
	group: Terreno
	group: Incolor
	group: Artefato
	group: Branco
	group: Azul
	group: Preto
	group: Vermelho
	group: Verde
	group: Multicolorido
	group: Híbrido
	colors:
		Branco			: rgb(255,231,185)
		Azul			: rgb(15,80,166)
		Preto			: rgb(33,33,33)
		Vermelho		: rgb(156,15,15)
		Verde			: rgb(68,144,15)
		Incolor			: rgb(101,104,94)
		Artefato		: rgb(109,132,158)
		Multicolorido	: rgb(244,177,10)
		Terreno			: rgb(103,64,35)
		Híbrido			: rgb(112,38,128)

statistics dimension:
	name: exact color
	position hint: 0010
	icon: stats/color.png
	description: The color as defined by the comprehensive rules. Does not take into account the back side or secondary face (like on adventures for example), except for split cards and aftermath cards.
	show empty: false
	script: if filter_from_statistic() == "Filtrado" or trim_from_statistic() then "" else prefixed_color_statistic()
	colors:
		​C		: rgb(101,104,94)					# There is a zero width space before the "C" so that it always appears first.
		​​W		: rgb(255,231,185)					# There are two zero width spaces before the "W" so that it always appear after the "C".
		​​​U		: rgb(15,80,166)					# There are three zero width spaces before the "U", etc...
		​​​​B		: rgb(22,22,22)
		​​​​​R		: rgb(156,15,15)
		​​​​​​G		: rgb(68,144,15)
