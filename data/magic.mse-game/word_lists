		is prefix: true
	word:
		script: " legendaria"
		is prefix: true
	word:
		script: " del mundo"
		is prefix: true
	word:
		script: " ficha"
		is prefix: true
		line below: true
	word: Conspiración
	word: Mazmorra
	word: Criatura élite
	word: Héroe
	word: Artefacto héroe
	word: Criatura anfitrión
	word: Plano
	word: Fenómeno
	word: Esquema
	word: Esquema continuo
	word: Vanguardia

