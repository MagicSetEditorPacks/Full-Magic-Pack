package:
	magic.mse-game:
		# Card fields
		name:		Имя
		cc:			Стоимость
		type:		Тип
		p/t:		С/З
		rarity:		Редкость
		card name:	Имя карты
		
		# Set info
		
		# descriptions/help text

	magic-mana-*.mse-symbol-font:
		# note: reminder/shortcut must start with a space, otherwise it is used as a shortcut
		#       so typing T *anywhere* would insert a symbol (which would be bad)
		menu item T:			Символ поворота	 T
		menu item Q:			Символ разворота	 Q
		menu item W:			Белая мана	 W
		menu item U:			Синяя мана	 U
		menu item B:			Черная мана	 B
		menu item R:			Красная мана	 R
		menu item G:			Зеленая мана	 G
