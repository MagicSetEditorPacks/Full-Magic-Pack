		is_tribal         : match@(match: "(?i)部族")
		is_enchantment    : match@(match: "(?i)结界")
		is_land           : match@(match: "(?i)地")
		is_planeswalker   : match@(match: "(?i)鵬洛客")
		is_spell          : match@(match: "(?i)瞬间|法术")
		is_instant        : match@(match: "(?i)瞬间")
		is_sorcery        : match@(match: "(?i)法术")

		is_nonstandard    : match@(match: "(?i)(诡局|地城|徽|英雄|现象|次元\\b|计划|衍生生物|先锋)")
		is_conspiracy     : match@(match: "(?i)诡局")
		is_dungeon        : match@(match: "(?i)地城")
		is_emblem         : match@(match: "(?i)徽")
		is_hero           : match@(match: "(?i)英雄")
		is_token          : match@(match: "(?i)衍生生物")
		is_phenomenon     : match@(match: "(?i)现象")
		is_plane		  : match@(match: "(?i)次元\\b")
		is_scheme         : match@(match: "(?i)计划")
		is_vanguard       : match@(match: "(?i)先锋")

		is_basic          : match@(match: "(?i)基本")
		is_elite          : match@(match: "(?i)精英")
		is_host           : match@(match: "(?i)精英")
		is_legendary      : match@(match: "(?i)传奇")
		is_ongoing        : match@(match: "(?i)正在进行的")
		is_snow           : match@(match: "(?i)雪境")
		is_world          : match@(match: "(?i)普世")
		get_supertypes    : filter_text@(match: "(?i)(传奇|基本|雪境|衍生生物|普世|正在进行的|精英|精英)\\b ?")
		remove_supertypes : replace_text@(match: "(?i)(传奇|基本|雪境|衍生生物|普世|正在进行的|精英|精英)\\b ?", replace:"")
		base_supertypes   : "传奇|基本|雪境|衍生生物|普世|正在进行的|精英|精英"

		is_plains         : match@(match: "(?i)平原")
		is_island         : match@(match: "(?i)海岛")
		is_swamp          : match@(match: "(?i)沼泽")
		is_mountain       : match@(match: "(?i)山脉")
		is_forest         : match@(match: "(?i)树林")
		is_wastes         : match@(match: "(?i)荒野")

		is_class		  : match@(match: "(?i)职业")
		is_nyx			  : match@(match: "(?i)结界(生物|神器)")
		is_saga			  : match@(match: "(?i)传纪")
		is_vehicle        : match@(match: "(?i)载具")
		is_lesson		  : match@(match: "(?i)课程")
		is_miracle		  : match@(input: keyword_usage(), match: "(?i)奇迹")
		is_mutate		  : match@(input: keyword_usage(), match: "(?i)合变")
		is_devoid		  : match@(input: keyword_usage(), match: "(?i)虚色")

		mana_symbol_production	: filter_text@(in_context:"(?i)(?<!增)加<match>", match:"<sym[^>]*>[^<]+<\/sym[^>]*>")
		gold_mana_production 	: filter_text@(in_context:"(?i)(?<!增)加<match>", match:"意颜色的法术力|不同颜色的法术力|其颜色组合由你选择")
		chosen_mana_production	: filter_text@(in_context:"(?i)(?<!增)加<match>", match:"该色的法术力")
