
############################################################## Magic word lists

### Basic Prefix Wordlists
### These use Prefix properly
word list:
	name: type-en
	word:
		name: Token 
		is prefix: true
	word:
		name: Legendary 
		is prefix: true
	word:
		name: Basic 
		is prefix: true
	word:
		name: Snow 
		is prefix: true
