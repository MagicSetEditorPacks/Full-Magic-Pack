		subtype_separator : " "
		keyword_types     : ["старый","ядро","экспертный","псевдо","действие","персонализированный"]

		is_artifact       : match@(match: "(?i)Артефакт")
		is_battle         : match@(match: "(?i)Боевой")
		is_creature       : match@(match: "(?i)Существо")
		is_kindred        : match@(match: "(?i)Племенной")
		is_tribal         : match@(match: "(?i)Племенной")
		is_enchantment    : match@(match: "(?i)Чары")
		is_land           : match@(match: "(?i)Земля")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Мгновенное заклинание|Волшебство")
		is_instant        : match@(match: "(?i)Мгновенное заклинание")
		is_sorcery        : match@(match: "(?i)Волшебство")

		is_nonstandard    : match@(match: "(?i)(Заговор|Подземелье|эмблему|Герой|Феномен|мир|Схема|фишк(ой|ку|ки|ке|ка)|Авангард)")
		is_conspiracy     : match@(match: "(?i)Заговор")
		is_dungeon        : match@(match: "(?i)Подземелье")
		is_emblem         : match@(match: "(?i)эмблему")
		is_hero           : match@(match: "(?i)Герой")
		is_token          : match@(match: "(?i)фишк(ой|ку|ки|ке|ка)")
		is_phenomenon     : match@(match: "(?i)Феномен")
		is_plane		  : match@(match: "(?i)мир")
		is_scheme         : match@(match: "(?i)Схема")
		is_vanguard       : match@(match: "(?i)Авангард")

		is_basic          : match@(match: "(?i)Базовая")
		is_elite          : match@(match: "(?i)Элита")
		is_host           : match@(match: "(?i)Хозяин")
		is_legendary      : match@(match: "(?i)Легендарн(ое|ые|ый|ая)")
		is_ongoing        : match@(match: "(?i)Текущая")
		is_snow           : match@(match: "(?i)Снежн(ое|ые|ый|ая)")
		is_world          : match@(match: "(?i)мировой")
		get_supertypes    : filter_text@(match: "(?i)(Легендарн(ое|ые|ый|ая)|Базовая|Снежн(ое|ые|ый|ая)|фишк(ой|ку|ки|ке|ка)|мировой|Текущая|Элита|Хозяин) ?")
		remove_supertypes : replace_text@(match: "(?i)(Легендарн(ое|ые|ый|ая)|Базовая|Снежн(ое|ые|ый|ая)|фишк(ой|ку|ки|ке|ка)|мировой|Текущая|Элита|Хозяин)[- ]?", replace:"")
		base_supertypes   : "Легендарн(ое|ые|ый|ая)|Базовая|Снежн(ое|ые|ый|ая)|фишк(ой|ку|ки|ке|ка)|мировой|Текущая|Элита|Хозяин"

		is_plains         : match@(match: "(?i)Равнина")
		is_island         : match@(match: "(?i)Остров")
