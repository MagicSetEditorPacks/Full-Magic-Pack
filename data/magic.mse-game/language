	"Português do Brasil" : [
		code              : "pt-br",
		spellcheck_code   : "pt_BR",
		fallback          : "English",
		pt_separator      : "/",
		type_separator    : " — ",
		subtype_separator : " ",
		keyword_types     : ["velho", "núcleo", "perito", "pseudo", "ação", "personalizado"]
		is_artifact       : match@(match: "(?i)Artefato")
		is_battle         : match@(match: "(?i)Batalha")
		is_creature       : match@(match: "(?i)Criatura")
		is_kindred        : match@(match: "(?i)Tribale")
		is_tribal         : match@(match: "(?i)Tribale")
		is_enchantment    : match@(match: "(?i)Encantamento")
		is_land           : match@(match: "(?i)Terreno")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Mágica Instantânea|Feitiço")
		is_instant        : match@(match: "(?i)Mágica Instantânea")
		is_sorcery        : match@(match: "(?i)Feitiço")

		is_nonstandard    : match@(match: "(?i)(Conspiração|Masmorra|Emblema|Herói|Fenômeno|Plano\\b|Esquema|Ficha de|Vanguarda)")
		is_conspiracy     : match@(match: "(?i)Conspiração")
		is_dungeon        : match@(match: "(?i)Masmorra")
		is_emblem         : match@(match: "(?i)Emblema")
		is_hero           : match@(match: "(?i)Herói")
		is_token          : match@(match: "(?i)Ficha de")
		is_phenomenon     : match@(match: "(?i)Fenômeno")
		is_plane		  : match@(match: "(?i)Plano\\b")
		is_scheme         : match@(match: "(?i)Esquema")
		is_vanguard       : match@(match: "(?i)Vanguarda")

		is_basic          : match@(match: "(?i)Básico")
		is_elite          : match@(match: "(?i)Elite")
		is_host           : match@(match: "(?i)Hospedar")
		is_legendary      : match@(match: "(?i)Lendário|Lendária")
		is_ongoing        : match@(match: "(?i)Continuo")
		is_snow           : match@(match: "(?i)da Neve")
		is_world          : match@(match: "(?i)do mundo")
		get_supertypes    : filter_text@(match: "(?i)(Lendário|Lendária|Básico|da Neve|Ficha de|do mundo|Continuo|Elite|Hospedar)\\b ?")
		remove_supertypes : replace_text@(match: "(?i)(Lendário|Lendária|Básico|da Neve|Ficha de|do mundo|Continuo|Elite|Hospedar)\\b ?", replace:"")
		base_supertypes   : "Lendário|Lendária|Básico|da Neve|Ficha de|do mundo|Continuo|Elite|Hospedar"

		is_plains         : match@(match: "(?i)Planície")
		is_island         : match@(match: "(?i)Ilha")
		is_swamp          : match@(match: "(?i)Pântano")
		is_mountain       : match@(match: "(?i)Montanha")
		is_forest         : match@(match: "(?i)Floresta")
		is_wastes         : match@(match: "(?i)Ermo")

		is_class		  : match@(match: "(?i)Classe")
		is_nyx			  : match@(match: "(?i) Encantamento")
		is_saga			  : match@(match: "(?i)Saga")
		is_vehicle        : match@(match: "(?i)Veículo")
		is_lesson		  : match@(match: "(?i)Lição")
		is_miracle		  : match@(input: keyword_usage(), match: "(?i)Milagre")
		is_mutate		  : match@(input: keyword_usage(), match: "(?i)Mutação")
		is_devoid		  : match@(input: keyword_usage(), match: "(?i)Desprovido")

		mana_symbol_production	: filter_text@(in_context:"(?i)\bAdicione [^(\n|^).]*<match>", match:"<sym[^>]*>[^<]+<\/sym[^>]*>")
		gold_mana_production 	: filter_text@(in_context:"(?i)\bAdicione [^(\n|^).]*<match>", match:"qualquer cor|cores diferentes")
		chosen_mana_production	: filter_text@(in_context:"(?i)\bAdicione [^(\n|^).]*<match>", match:"daquela cor|cor(es)? escolhidas?")
