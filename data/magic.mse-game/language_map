		type_separator    : " — "
		subtype_separator : "<atom-sep>, </atom-sep>"
		keyword_types     : ["alt","Kern","Experte","Pseudo","Aktion","personalisiert"]

		is_artifact       : match@(match: "(?i)Artefakt")
		is_battle         : match@(match: "(?i)Schlacht")
		is_creature       : match@(match: "(?i)Kreatur")
		is_kindred        : match@(match: "(?i)Stammes-")
		is_tribal         : match@(match: "(?i)Stammes-")
		is_enchantment    : match@(match: "(?i)Verzauberung")
		is_land           : match@(match: "(?i)Land")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Spontanzauber|Hexerei")
		is_instant        : match@(match: "(?i)Spontanzauber")
		is_sorcery        : match@(match: "(?i)Hexerei")

		is_nonstandard    : match@(match: "(?i)(Verschwörung|Gewölbe|Emblem|Held|Phänomen|Welt|Planen|Spielstein|Vorhut)")
		is_conspiracy     : match@(match: "(?i)Verschwörung")
		is_dungeon        : match@(match: "(?i)Gewölbe")
		is_emblem         : match@(match: "(?i)Emblem")
		is_hero           : match@(match: "(?i)Held")
		is_token          : match@(match: "(?i)Spielstein")
		is_phenomenon     : match@(match: "(?i)Phänomen")
		is_plane		  : match@(match: "(?i)Welt")
		is_scheme         : match@(match: "(?i)Planen")
		is_vanguard       : match@(match: "(?i)Vorhut")

		is_basic          : match@(match: "(?i)Standard")
		is_elite          : match@(match: "(?i)Elite")
		is_host           : match@(match: "(?i)Gastgeber")
		is_legendary      : match@(match: "(?i)Legendäre(r|s)?")
		is_ongoing        : match@(match: "(?i)Laufendes")
		is_snow           : match@(match: "(?i)Verschneite(r|s)?")
		is_world          : match@(match: "(?i)Welten")
		get_supertypes    : filter_text@(match: "(?i)(Legendäre(r|s)?|Standard|Verschneite(r|s)?|Spielstein|Welten|Laufendes|Elite|Gastgeber) ?")
		remove_supertypes : replace_text@(match: "(?i)(Legendäre(r|s)?|Standard|Verschneite(r|s)?|Spielstein|Welten|Laufendes|Elite|Gastgeber)[- ]?", replace:"")
		base_supertypes   : "Legendäre(r|s)?|Standard|Verschneite(r|s)?|Spielstein|Welten|Laufendes|Elite|Gastgeber"

