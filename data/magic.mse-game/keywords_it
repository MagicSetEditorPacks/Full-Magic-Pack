keyword:
	#english keyword: Replicate
	keyword: Replicare
	match: Replicare <atom-param>costo</atom-param>
	mode: expert
	reminder: Quando lanci questa magia, copiala per ogni volta che hai pagato il suo costo di replica. Puoi scegliere nuovi bersagli per le copie.
	#example: Lose Focus

keyword:
	#english keyword: Forecast
	keyword: Previsione
	match: Previsione — <atom-param>costo</atom-param>
	mode: expert
	reminder: Attiva questa abilità solo durante il tuo mantenimento e solo una volta per turno.
	#example: Piercing Rays

