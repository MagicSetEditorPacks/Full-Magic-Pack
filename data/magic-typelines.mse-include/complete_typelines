card style:
	indicator:
		left:				{ indicator_left() }
		top:				{ indicator_top()  }
		width:				{ indicator_size() }
		height:				{ indicator_size() }
		z index:			1200
		render style:		image
		visible:			{ (not indicator_disabled()) and has_identity() }
		image:				{ card_identity(card.indicator) }
	type:
		left:				{ indicator_left() + (if (not indicator_disabled()) and has_identity() then indicator_size()*1.15 else 0) + type_offset_left() }
		top:				{ indicator_top() - indicator_size() * 0.2 + type_offset_top() }
		right:				{ rarity_left() + if set.shorten_types_for_rarity then (-rarity_size()*0.09) else rarity_size() + type_offset_right() }
		height:				{ indicator_size() * 1.3 + type_offset_height() }
		z index:			1210
		alignment:			middle left shrink-overflow
		visible:			{ not type_disabled() }
		font:
			name: 				{ type_font() }
			italic name: 		{ type_font_italic() }
			size: 				{ type_font_size() }
			color: 				{ type_font_color() }
			separator color:	red
	rarity:
		left:				{ rarity_left() }
		top:				{ rarity_top() }
		width:				{ rarity_size() }
		height:				{ rarity_size() }
		z index:			1200
		alignment:			middle center
		visible:			{ not rarity_disabled() }
		render style:		image
		choice images:
			# Images based on the set symbol
			basic land:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "c.png"
					else if alt_rarity() then alt_symbol()
					else if styling.inverted_common_symbol then symbol_variation(symbol: set.symbol, variation: "invertedcommon")
					else symbol_variation(symbol: set.symbol, variation: "common")
			common:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "c.png"
					else if alt_rarity() then alt_symbol()
					else if styling.inverted_common_symbol then symbol_variation(symbol: set.symbol, variation: "invertedcommon")
					else symbol_variation(symbol: set.symbol, variation: "common")
			uncommon:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "u.png"
					else if alt_rarity() then alt_symbol()
					else symbol_variation(symbol: set.symbol, variation: "uncommon")
			rare:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "r.png"
					else if alt_rarity() then alt_symbol()
					else symbol_variation(symbol: set.symbol, variation: "rare")
			mythic rare:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "m.png"
					else if alt_rarity() then alt_symbol()
					else symbol_variation(symbol: set.symbol, variation: "mythic rare")
			special:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "s.png"
					else if alt_rarity() then alt_symbol()
					else symbol_variation(symbol: set.symbol, variation: "special")
			masterpiece:
				script:
					if use_main_rarity() then "/magic-mainframe-extras.mse-include/" + un_png(set.mainframe_rarity_name) + "mp.png"
					else symbol_variation(symbol: set.masterpiece_symbol, variation: "mythic rare")
	indicator 2:
		left:				{ indicator_left_2() }
		top:				{ indicator_top_2() }
		width:				{ indicator_size_2() }
		height:				{ indicator_size_2() }
		z index:			1200
		render style:		image
		visible:			{ (not indicator_disabled_2()) and has_identity_2() }
		image:				{ card_identity(card.indicator_2) }
	type 2:
		left:				{ indicator_left_2() + (if (not indicator_disabled_2()) and has_identity_2() then indicator_size_2()*1.15 else 0) + type_offset_left_2() }
		top:				{ indicator_top_2() - indicator_size_2() * 0.2 + type_offset_top_2() }
		right:				{ rarity_left_2() + if set.shorten_types_for_rarity then (-rarity_size_2()*0.09) else rarity_size_2() + type_offset_right_2() }
		height:				{ indicator_size_2() *1.3 + type_offset_height_2() }
		z index:			1210
		alignment:			middle left shrink-overflow
		visible:			{ not type_disabled_2() }
		font:
			name: 				{ type2_font() }
			italic name: 		{ type2_font_italic() }
			size: 				{ type2_font_size() }
			color: 				{ type2_font_color() }
			separator color:	red
	rarity 2:
		left:				{ rarity_left_2() }
		top:				{ rarity_top_2() }
		width:				{ rarity_size_2() }
		height:				{ rarity_size_2() }
		z index:			1200
		alignment:			middle center
		visible:			{ not rarity_disabled_2() }
		render style:		image
		choice images:
			# Images based on the set symbol
			basic land:
