
############################################################## Blame GenevensiS for this

has keywords: true

############################################################## Keyword mode

keyword mode:
	name: old
	#name: vieux
	description: Vieux mots-clefs (Regroupement, Déphasage, etc.)

keyword mode:
	name: core
	#name: noyau
	description: Mots-clefs basiques (Vol, Célérité, etc.)

keyword mode:
	name: expert
	#name: expert
	description: Mots-clefs experts (Déluge, Dragage, etc.)

keyword mode:
	name: pseudo
	#name: pseudo
	description: Pseudo mots-clefs (Toucheterre, Seuil, etc.)

keyword mode:
	name: action
	#name: action
	description: Mots-clefs d'actions (Proliférez, Régénérez, etc.)

keyword mode:
	name: custom
	#name: personnalisé
	description: Mots-clefs personnalisés
	is default: true

############################################################## Keyword parameter types

keyword parameter type:
	name: coût
	match: [ ][HSVECTQXYZI0-9WUBRG/|]*|[-—―～][^(\r\n\.。:：]*
	script: cost_parameter_script()
	reminder script: cost_parameter_script()
	separator before is: ( |- ?|— ?|― ?|～)
	separator after is: [.。:：]
	separator script: cost_parameter_dash()

keyword parameter type:
	name: nombre
	match: [XYZ0-9０１２３４５６７８９]+

keyword parameter type:
	name: préfixe
	match: [^(.,”":;\n—]+
	optional: false

keyword parameter type:
	name: un_mot
	match: [^(.,”":;\n— ]+
	optional: false

keyword parameter type:
	name: nom
	match: ([^(.:;\n—]+|[ ])
	optional: true

############################################################## All Magic keywords

keyword:
	#english keyword: Poisonous
	keyword: Empoisonnement
	match: Empoisonnement <atom-param>nombre</atom-param>
	mode: expert
	reminder: À chaque fois que cette creature inflige des blessures de combat à un joueur, ce joueur gagne {french_number(param1)} marqueurs « poison ». Un joueur qui a au moins dix marqueurs « poison » perd la partie.
	#example: Snake Cult Initiation

keyword:
	#english keyword: Deathtouch
	keyword: Contact mortel
	match: Contact mortel
	mode: core
	reminder: Le nombre de blessures que cette créature inflige à une créature, quel qu'il soit, est suffisant pour la détruire.
	#example: Sedge Scorpion

keyword:
	#english keyword: Defender
	keyword: Défenseur
	match: Défenseur
	mode: core
	reminder: Cette créature ne peut pas attaquer.
	#example: Moat Piranhas

keyword:
	#english keyword: Double strike
	keyword: Double initiative
	match: Double initiative
	mode: core
	reminder: Cette créature inflige des blessures de combat d'initiative et des blessures de combat normales.
	#example: Iroas's Champion

keyword:
	#english keyword: Enchant
	keyword: Enchanter
	match: Enchanter : <atom-param>nom</atom-param>
	mode: core
	reminder: Ciblez un/une {param1} au moment où vous jouez cette carte. Cette carte arrive sur le champ de bataille attachée à ce/cette {param1}.
	#example: Contaminated Bond

keyword:
	#english keyword: Equip
	keyword: Équipement
	match: Équipement <atom-param>coût</atom-param>
	mode: core
	reminder: {param1} : Attachez à une créature ciblée que vous contrôlez. N'attachez l'équipement que lorsque vous pourriez lancer un rituel.
	#example: Greatsword of Tyr

keyword:
	#english keyword: First strike
	keyword: Initiative
	match: Initiative
	mode: core
	reminder: Cette créature inflige des blessures de combat avant les créatures sans l'initiative.
	#example: Warclamp Mastiff

keyword:
	#english keyword: Flashback
	keyword: Flashback
	match: Flashback <atom-param>coût</atom-param>
	mode: expert
	reminder: Vous pouvez lancer cette carte depuis votre cimetière pour son coût de flashback. Exilez-la ensuite.
	#example: Rite of Harmony

keyword:
	#english keyword: Flash
	keyword: Flash
	match: Flash
	mode: core
	reminder: Vous pouvez lancer ce sort à tout moment où vous pourriez lancer un éphémère.
	#example: Hired Blade

keyword:
	#english keyword: Flying
	keyword: Vol
	match: Vol
	mode: core
	reminder: Cette créature ne peut pas être bloquée excepté par des créatures avec le vol ou la portée.
	#example: Bartizan Bats

keyword:
	#english keyword: Haste
	keyword: Célérité
	match: Célérité
	mode: core
	reminder: Cette créature peut attaquer et <sym-auto>T</sym-auto> dès le tour où elle arrive sous votre contrôle.
	#example: Hostile Minotaur

keyword:
	#english keyword: Hexproof
	keyword: Défense talismanique
	match: Défense talismanique
	mode: core
	reminder: Ce permanent ne peut pas être la cible de sorts ou de capacités que vos adversaires contrôlent.
	#example: Humongulus

keyword:
	#english keyword: Indestructible
	keyword: Indestructible
	match: Indestructible
	mode: core
	reminder: {if languages["Français"].is_creature(card.type) then "Les blessures et les effets qui disent « détruisez » ne détruisent pas cette créature. Si son endurance est inférieure ou égale à 0, elle est toujours mise dans le cimetière de son propriétaire." else "Les effets qui disent « détruisez » ne détruisent pas ce permanent."}
	#example: Seraph of the Suns

keyword:
	#english keyword: Intimidate
	keyword: Intimidation
	match: Intimidation
	mode: old
	reminder: Cette créature ne peut pas être bloquée excepté par des créatures-artefacts et/ou des créatures qui partagent une couleur avec elle.
	#example: Krenko's Enforcer

keyword:
	#english keyword: Landwalk
	keyword: Traversée des terrains
	match: Traversée des <atom-param>nom</atom-param>
	mode: old
	reminder: Cette créature ne peut pas être bloquée tant que le joueur défenseur contrôle au moins un/une {param1}.
	#example: Farbog Explorer

keyword:
	#english keyword: Landwalk
	keyword: Traversée des terrains
	match: Traversée de <atom-param>nom</atom-param>
	mode: old
	reminder: Cette créature ne peut pas être bloquée tant que le joueur défenseur contrôle au moins un/une {param1}.
	#example: Farbog Explorer

keyword:
	#english keyword: Lifelink
	keyword: Lien de vie
	match: Lien de vie
	mode: core
	reminder: Les blessures infligées par cette créature vous font aussi gagner autant de points de vie.
	#example: Daybreak Chaplain

keyword:
	#english keyword: Ward
	keyword: Parade
	match: Parade <atom-param>coût</atom-param>
	mode: expert
	reminder: À chaque fois que cette créature devient la cible d'un sort ou d'une capacité qu'un adversaire contrôle, contrecarrez ce sort ou cette capacité à moins que ce joueur ne paie {param1}.
	#example: Rimeshield Frost Giant

keyword:
	#english keyword: Protection
	keyword: Protection
	match: Protection contre les <atom-param>nom</atom-param>
	mode: expert
	reminder: Ce permanent ne peut pas être bloqué, ciblé, blessé, enchanté ou équipé par quoi que ce soit de {param1}.
	#example: Blightbeetle

keyword:
	#english keyword: Protection
	keyword: Protection
	match: Protection contre la <atom-param>nom</atom-param>
	mode: expert
	reminder: Ce permanent ne peut pas être bloqué, ciblé, blessé, enchanté ou équipé par quoi que ce soit de {param1}.
	#example: Blightbeetle

keyword:
	#english keyword: Protection
	keyword: Protection
	match: Protection contre le <atom-param>nom</atom-param>
	mode: expert
	reminder: Ce permanent ne peut pas être bloqué, ciblé, blessé, enchanté ou équipé par quoi que ce soit de {param1}.
	#example: Blightbeetle

