		is_instant        : match@(match: "(?i)Éphémère")
		is_sorcery        : match@(match: "(?i)Rituel")

		is_nonstandard    : match@(match: "(?i)(Conspiration|Donjon|Emblème|Héros|Phénomène|Plan\\b|Schème|Jeton de|Avant-garde)")
		is_conspiracy     : match@(match: "(?i)Conspiration")
