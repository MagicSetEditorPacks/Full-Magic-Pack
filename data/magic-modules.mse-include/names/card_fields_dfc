		font:
			name:				{ name2_font() }
			italic name:		{ name2_font_italic() }
			size:				{ name2_font_size() }
			color:				{ name2_font_color() }
			shadow color:			{ name2_shadow_color() }
			shadow displacement x:	-2
			shadow displacement y:	2
			#shadow displacement x:	{ name2_shadow_displacement_x() }
