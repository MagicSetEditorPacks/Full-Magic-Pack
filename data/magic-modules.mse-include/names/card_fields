card style:
	name:
		left:				{ name_left() + name_transform_symbol_shift() + name_card_symbol_shift() + nameline_offset_left() + nameline_offset_height() }
		top:				{ name_top() + nameline_offset_top() + name_font_vertical() }
		right:				{ name_right() + nameline_offset_width() }
		height:				{ name_height() + nameline_offset_height() }
		alignment:			middle left shrink-overflow
		visible:			{ (not nameline_disabled()) and not name_disabled() }
		font:
			name:				{ name_font() }
			italic name:		{ name_font_italic() }
			size:				{ name_font_size() }
			color:				{ name_font_color() }
		z index:			1210