		type_separator		: " — ",
		subtype_separator	: " ",
		keyword_types		: ["viejo", "núcleo", "experto", "seudo", "acción", "personalizado"]
		is_artifact       : match@(match: "(?i)Artefacto")
		is_battle         : match@(match: "(?i)Baralla")
		is_creature       : match@(match: "(?i)Criatura")
		is_kindred        : match@(match: "(?i)Tribal")
		is_tribal         : match@(match: "(?i)Tribal")
		is_enchantment    : match@(match: "(?i)Encantamiento")
		is_land           : match@(match: "(?i)Tierra")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Instantáneo|Conjuro")
		is_instant        : match@(match: "(?i)Instantáneo")
		is_sorcery        : match@(match: "(?i)Conjuro")

		is_nonstandard    : match@(match: "(?i)(Conspiración|Mazmorra|Emblema|Héroe|Fenómeno|Plano\\b|Esquema|Ficha de|Vanguardia)")
		is_conspiracy     : match@(match: "(?i)Conspiración")
		is_dungeon        : match@(match: "(?i)Mazmorra")
		is_emblem         : match@(match: "(?i)Emblema")
		is_hero           : match@(match: "(?i)Héroe")
		is_token          : match@(match: "(?i)Ficha de")
		is_phenomenon     : match@(match: "(?i)Fenómeno")
		is_plane		  : match@(match: "(?i)Plano\\b")
		is_scheme         : match@(match: "(?i)Esquema")
		is_vanguard       : match@(match: "(?i)Vanguardia")

		is_basic          : match@(match: "(?i)básica")
		is_elite          : match@(match: "(?i)Élite")
		is_host           : match@(match: "(?i)Anfitrión")
		is_legendary      : match@(match: "(?i)Legendario|Legendaria")
		is_ongoing        : match@(match: "(?i)Continuo")
		is_snow           : match@(match: "(?i)nevado|nevada")
		is_world          : match@(match: "(?i)del mundo")
		get_supertypes    : filter_text@(match: "(?i)(Legendario|Legendaria|básica|nevado|nevada|Ficha de|del mundo|Continuo|Élite|Anfitrión)\\b ?")
