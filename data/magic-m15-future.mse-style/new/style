	############################# Copyright stuff
	illustrator:
		left: { 44 + card_style.set_code.content_width }
		top : 497.5
		width: 200
		height: 10
		z index: 4
		font:
			name: Beleren Small Caps Bold
			size: 7.5
			color: white
	copyright:
		right: 350
		top : { if card.loyalty != "" then 495 else 488 }
		width: 140
		height: 10
		visible: {not set.automatic_copyright}
		alignment: bottom right
		z index: 4
		font:
			name: Matrix
			size: 7
