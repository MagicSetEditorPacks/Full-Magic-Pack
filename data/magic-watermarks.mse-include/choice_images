	"set symbol":
		{
			dark_textbox :=		(face == 1 and invert_watermark())
							or	(face == 2 and invert_watermark2())
							or	(face == 3 and invert_watermark3())
			base_image := drop_shadow(
				offset_x: -0.02,
				offset_y: 0.03,
				blur_radius: 0.02,
				alpha: 1,
				color: rgb(128,128,128),
				enlarge(
					border_size: 0.03,
					symbol_variation(
						symbol: set.symbol,
						variation: "watermark"
					)
				)
			)
			if not dark_textbox then base_image := invert_image(base_image)
			set_alpha(alpha: 0.25, base_image)
		}
]