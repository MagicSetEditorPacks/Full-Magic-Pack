card style:
	indicator:
		left:				{ indicator_left() + typeline_offset_left() }
		top:				{ indicator_top() + typeline_offset_top() }
		width:				{ indicator_size() + typeline_offset_height() }
		height:				{ indicator_size() + typeline_offset_height() }
		z index:			1200
		render style:		image
		visible:			{ (not typeline_disabled()) and (not indicator_disabled()) and has_identity() }
		image:				{ card_identity(card.indicator) }