statistics dimension:
	name: casting cost
	position hint: 0100
	icon: stats/casting_cost.png
	description: The casting cost. Counts each side/face separately if the option is checked in the Set info tab.
	split list: true
	show empty: false
	script: if filter_from_statistic() == "거르는" or trim_from_statistic() then "" else casting_cost_statistic()

statistics dimension:
	name: mana value
	position hint: 0110
	icon: stats/mana_value.png
	description: The mana value. Counts each side/face separately if the option is checked in the Set info tab, except for split cards and aftermath cards, for which it counts the sum of all faces.
	numeric: true
	show empty: false
	script: if filter_from_statistic() == "거르는" or trim_from_statistic() then "" else mana_value_statistic()

statistics dimension:
	name: permanent/non
	position hint: 0200
	icon: stats/type.png
	description: Counts the number of creatures, noncreature permanents, and nonpermanents. Counts each side/face separately.
	split list: true
	show empty: false
	script: if filter_from_statistic() == "거르는" or trim_from_statistic() then "" else permanent_statistic()
	colors:
		​생물			: rgb(15,80,166)
		​​지속물		: rgb(18,105,166)
		​​​비지속물		: rgb(156,15,15)
		​​​​지휘 존		: rgb(244,227,21)
		​​​​​알려지지 않은	: rgb(33,33,33)

statistics dimension:
	name: supertype
	position hint: 0210
	icon: stats/type.png
	description: The card’s supertype, not including types. Counts each side/face separately.
	split list: true
	show empty: false
	script: if filter_from_statistic() == "거르는" or trim_from_statistic() then "" else supertype_statistic()
#	group: ​기본
#	group: 눈
#	group: ​​​만능인
#	group: 전설적
#	group: 토큰
#	group: ​​​​​​진행중인
#	group: 엘리트
#	group: 주인
	colors:
		​기본			: rgb(103,64,35)			# There is a zero width space before the "Basic" so that it always appears after custom types.
		​​눈			: rgb(109,132,158)			# There are two zero width spaces before the "Snow" so that it always appears after basics.
		​​​만능인		: rgb(120,100,180)			# There are three zero width spaces before the "World", etc...
		​​​​전설적		: rgb(70,49,125)
		​​​​​토큰			: rgb(15,80,166)
		​​​​​​진행중인		: rgb(56,169,138)
		​​​​​​​엘리트		: rgb(195,221,62)
		​​​​​​​​주인			: rgb(157,168,115)

