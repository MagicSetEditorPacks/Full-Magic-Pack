	Italiano: [
		code              : "it",
		spellcheck_code   : "it_IT",
		fallback          : "English",
		pt_separator      : "/",
		type_separator    : " — ",
		subtype_separator : " ",
		keyword_types     : ["vecchio", "nucleo", "esperto", "pseudo", "azione", "personalizzato"]
		is_artifact       : match@(match: "(?i)Artefatto")
		is_battle         : match@(match: "(?i)Battaglia")
		is_creature       : match@(match: "(?i)Creatura")
		is_kindred        : match@(match: "(?i)Tribale")
		is_tribal         : match@(match: "(?i)Tribale")
		is_enchantment    : match@(match: "(?i)Incantesimo")
		is_land           : match@(match: "(?i)Terra")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Istantaneo|Stregoneria")
		is_instant        : match@(match: "(?i)Istantaneo")
		is_sorcery        : match@(match: "(?i)Stregoneria")

		is_nonstandard    : match@(match: "(?i)(Cospirazione|Dungeon|Emblema|Eroe|Fenomeno|Piano\\b|Schema|Pedina|Avanguardia)")
		is_conspiracy     : match@(match: "(?i)Cospirazione")
		is_dungeon        : match@(match: "(?i)Dungeon")
		is_emblem         : match@(match: "(?i)Emblema")
		is_hero           : match@(match: "(?i)Eroe")
		is_token          : match@(match: "(?i)Pedina")
		is_phenomenon     : match@(match: "(?i)Fenomeno")
		is_plane		  : match@(match: "(?i)Piano\\b")
		is_scheme         : match@(match: "(?i)Schema")
		is_vanguard       : match@(match: "(?i)Avanguardia")

		is_basic          : match@(match: "(?i)Base")
		is_elite          : match@(match: "(?i)Elite")
		is_host           : match@(match: "(?i)Ospite")
		is_legendary      : match@(match: "(?i)Leggendario|Leggendaria")
		is_ongoing        : match@(match: "(?i)in Corso")
		is_snow           : match@(match: "(?i)Neve")
		is_world          : match@(match: "(?i)del mondo")
		get_supertypes    : filter_text@(match: "(?i)(Leggendario|Leggendaria|Base|Neve|Pedina|del mondo|in Corso|Elite|Ospite)\\b ?")
		remove_supertypes : replace_text@(match: "(?i)(Leggendario|Leggendaria|Base|Neve|Pedina|del mondo|in Corso|Elite|Ospite)\\b ?", replace:"")
		base_supertypes   : "Leggendario|Leggendaria|Base|Neve|Pedina|del mondo|in Corso|Elite|Ospite"

		is_plains         : match@(match: "(?i)Pianura")
		is_island         : match@(match: "(?i)Isola")
		is_swamp          : match@(match: "(?i)Palude")
		is_mountain       : match@(match: "(?i)Montagna")
		is_forest         : match@(match: "(?i)Foresta")
		is_wastes         : match@(match: "(?i)Distesa")

		is_class		  : match@(match: "(?i)Classe")
		is_nyx			  : match@(match: "(?i) Incantesimo")
		is_saga			  : match@(match: "(?i)Saga")
		is_vehicle        : match@(match: "(?i)Veicolo")
		is_lesson		  : match@(match: "(?i)Lezione")
		is_miracle		  : match@(input: keyword_usage(), match: "(?i)Miracolo")
		is_mutate		  : match@(input: keyword_usage(), match: "(?i)Mutazione")
		is_devoid		  : match@(input: keyword_usage(), match: "(?i)Vacuità")

		mana_symbol_production	: filter_text@(in_context:"(?i)\bAggiungi [^(\n|^).]*<match>", match:"<sym[^>]*>[^<]+<\/sym[^>]*>")
		gold_mana_production 	: filter_text@(in_context:"(?i)\bAggiungi [^(\n|^).]*<match>", match:"qualsiasi colore|colori diversi|qualsiasi combinazione di colori")
		chosen_mana_production	: filter_text@(in_context:"(?i)\bAggiungi [^(\n|^).]*<match>", match:"quel colore|color[ei] scelt[oi]")
		word_list		  : "word_list_it"
		word_list_artifact:
			 "Attraction,"
			+ "Cibo,"
			+ "Congegno,"
			+ "Equipaggiamento,"
			+ "Fortificazione,"
			+ "Incubatrice,"
			+ "Indizio,"
