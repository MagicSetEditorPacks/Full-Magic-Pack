card field:
	type: text
	name: card number
	description: The cardnumber of the card.
	show statistics: false
	save value: false
	editable: false
	card list visible: true
	card list name: #
	card list column: 0600
	card list alignment: right
	card list width: 90
	script: card_number() + "/" + card_count()
	sort script: rarity_sort() + card.card_number

