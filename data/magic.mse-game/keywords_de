
############################################################## Blame GenevensiS for this

has keywords: true

############################################################## Keyword mode

keyword mode:
	name: old
	#name: alt
	description: Old keywords (Banding, Phasing, etc.)

keyword mode:
	name: core
	#name: Kern
	description: Core set keywords (Flying, Haste, etc.)

keyword mode:
	name: expert
	#name: Experte
	description: Expert set keywords (Storm, Dredge, etc.)

keyword mode:
	name: pseudo
	#name: Pseudo
	description: Pseudo keyword / Ability words (Landfall, Threshold, etc.)

keyword mode:
	name: action
	#name: Aktion
	description: Keyword actions, reminder text at end of line (Scry, Regenerate, etc.)

keyword mode:
	name: custom
	#name: personalisiert
	description: Custom keywords
	is default: true

############################################################## Keyword parameter types

keyword parameter type:
	name: Kosten
	match: [ ]?[HSVECTQXYZI0-9WUBRG/|]+|[-—―～][^(\r\n\.。:：]*
	optional: true
	script: cost_parameter_script()
	reminder script: cost_parameter_script()
	separator before is: ( |- ?|— ?|― ?|～|)
	separator after is: [.。:：]
	separator script: cost_parameter_dash()

keyword parameter type:
	name: Nummer
	match: [XYZ0-9０１２３４５６７８９]+
	optional: true

keyword parameter type:
	name: Präfix
	match: [^(.,”":;\n—]+
	optional: false

keyword parameter type:
	name: Name
	match: ([^(.:;\n—]+|[ ])
	optional: true

############################################################## All Magic keywords

keyword:
	#english keyword: Poisonous
	keyword: Gift
	match: Gift <atom-param>Nummer</atom-param>
	mode: expert
	reminder: Immer wenn diese Kreatur einem Spieler Kampfschaden zufügt, erhält dieser Spieler {german_number(param1)} Giftmarken. Ein Spieler mit zehn oder mehr Giftmarken verliert das Spiel.
	#example: Snake Cult Initiation

keyword:
	#english keyword: Deathtouch
	keyword: Todesberührung
	match: Todesberührung
	mode: core
	reminder: Eine beliebige Menge Schadenspunkte, die er einer Kreatur zufügt, sind ausreichend, um diese zu zerstören.
	#example: Sedge Scorpion

keyword:
	#english keyword: Defender
	keyword: Verteidiger
	match: Verteidiger
	mode: core
	reminder: Diese Kreatur kann nicht angreifen.
	#example: Moat Piranhas

keyword:
	#english keyword: Double strike
	keyword: Doppelschlag
	match: Doppelschlag
	mode: core
	reminder: Diese Kreatur fügt sowohl Erstschlags- als auch normalen Kampfschaden zu.
	#example: Iroas's Champion

keyword:
	#english keyword: Enchant
	keyword: Verzaubert
	match: Verzaubert <atom-param>Name</atom-param>
	mode: core
	reminder: Bestimme {param1} als Ziel, sowie du diese Karte spielst. Diese Karte kommt an der/die/das {remove_ein(param1)} angelegt ins Spiel.
	#example: Contaminated Bond

keyword:
	#english keyword: Equip
	keyword: Ausrüsten
	match: Ausrüsten <atom-param>Kosten</atom-param>
	mode: core
	reminder: {param1}: Lege diese Karte an eine Kreatur deiner Wahl an, die du kontrollierst. Spiele Ausrüsten wie eine Hexerei.
	#example: Greatsword of Tyr

keyword:
	#english keyword: First strike
	keyword: Erstschlag
	match: Erstschlag
	mode: core
	reminder: Diese Kreatur fügt Kampfschaden vor Kreaturen ohne Erstschlag zu.
	#example: Warclamp Mastiff

keyword:
	#english keyword: Flashback
	keyword: Rückblende
	match: Rückblende <atom-param>Kosten</atom-param>
	mode: expert
	reminder: Du kannst diese Karte aus deinem Friedhof für ihre Rückblendekosten wirken. Schicke sie danach ins Exil.
	#example: Rite of Harmony

keyword:
	#english keyword: Flash
	keyword: Aufblitzen
	match: Aufblitzen
	mode: core
	reminder: Du kannst diesen Zauberspruch zu jedem Zeitpunkt wirken, zu dem du einen Spontanzauber wirken könntest.
	#example: Hired Blade

keyword:
	#english keyword: Flying
	keyword: Fliegend
	match: Fliegend
	mode: core
	reminder: Diese Kreatur kann außer von fliegenden Kreaturen und Kreaturen mit Reichweite nicht geblockt werden.
	#example: Bartizan Bats

keyword:
	#english keyword: Haste
	keyword: Eile
	match: Eile
	mode: core
	reminder: Diese Kreatur kann angreifen und <sym-auto>T</sym-auto>, sobald sie unter deine Kontrolle kommt.
	#example: Hostile Minotaur

keyword:
	#english keyword: Hexproof
	keyword: Fluchsicher
	match: Fluchsicher
	mode: core
	reminder: Diese bleibende Karte kann nicht das Ziel von Zaubersprüchen oder Fähigkeiten sein, die deine Gegner kontrollieren.
	#example: Humongulus

keyword:
	#english keyword: Indestructible
	keyword: Unzerstörbar
	match: Unzerstörbar
	mode: core
	reminder: Schaden und Effekte, die „zerstören", zerstören {if languages["Deutsch"].is_creature(card.type) then "diese Kreatur" else "diese bleibende Karte"} nicht.{if languages["Deutsch"].is_creature(card.type) then " Falls ihre Widerstandskraft </sym-auto>0<sym-auto> oder weniger beträgt, wird sie dennoch auf den Friedhof ihres Besitzers gelegt." else ""}
	#example: Seraph of the Suns

keyword:
	#english keyword: Intimidate
	keyword: Einschüchtern
	match: Einschüchtern
	mode: old
	reminder: Diese Kreatur kann außer von Artefaktkreaturen und/oder Kreaturen, mit denen sie eine Farbe gemeinsam hat, nicht geblockt werden.
	#example: Krenko's Enforcer

keyword:
	#english keyword: Landwalk
	keyword: Landtarnung
	match: <atom-param>Präfix</atom-param>arnung
	mode: old
	reminder: Diese Kreatur kann nicht geblockt werden, solange der verteidigende Spieler ein/eine {param1} kontrolliert.
	#example: Farbog Explorer

keyword:
	#english keyword: Lifelink
	keyword: Lebensverknüpfung
	match: Lebensverknüpfung
	mode: core
	reminder: Schaden, der von dieser Kreatur zugefügt wird, lässt dich entsprechend viele Lebenspunkte dazuerhalten.
	#example: Daybreak Chaplain

keyword:
	#english keyword: Ward
	keyword: Abwehr
	match: Abwehr <atom-param>Kosten</atom-param>
	mode: expert
	reminder: Immer wenn diese Kreatur das Ziel eines Zauberspruchs oder einer Fähigkeit wird, den bzw. die ein Gegner kontrolliert, neutralisiere ihn bzw. sie, es sei denn, jener Spieler bezahlt {param1}.
	#example: Rimeshield Frost Giant

keyword:
	#english keyword: Protection
	keyword: Schutz
	match: Schutz vor <atom-param>Name</atom-param>
	mode: expert
	reminder: Diese bleibende Karte kann von nichts {param1} geblockt oder als Ziel bestimmt werden, von ihm Schaden zugefügt bekommen oder von ihm verzaubert oder ausgerüstet werden.
	#example: Blightbeetle

keyword:
	#english keyword: Reach
	keyword: Reichweite
	match: Reichweite
	mode: core
	reminder: Diese Kreatur kann fliegende Kreaturen blocken.
	#example: Canopy Spider

keyword:
	#english keyword: Shroud
	keyword: Verhüllt
	match: Verhüllt
	mode: core
	reminder: Diese bleibende Karte kann nicht das Ziel von Zaubersprüchen oder Fähigkeiten sein.
	#example: Nimble Mongoose

keyword:
	#english keyword: Trample
	keyword: Trampelschaden
	match: Trampelschaden
	mode: core
	reminder: Diese Kreatur kann überschüssigen Kampfschaden dem verteidigenden Spieler oder Planeswalker zufügen.
	#example: Colossal Dreadmaw

keyword:
	#english keyword: Vigilance
	keyword: Wachsamkeit
	match: Wachsamkeit
	mode: core
	reminder: Diese Kreatur wird beim Angreifen nicht getappt.
	#example: Alpine Watchdog

