# Watermark images for templates with a very bright textbox background

choice images:
	set symbol:
		script:
			base := drop_shadow(offset_x: -0.02, offset_y: 0.03, blur_radius: 0.02, alpha: 1, color: rgb(128,128,128),
						enlarge(border_size: 0.03,
							symbol_variation(symbol: set.symbol, variation: "watermark")
						)
					)
			#if set.blend_with_colors
			#then spotlight_watermark_blend(alpha: 0.4, base, l:106, h:106) else
			invert_image(set_alpha(alpha: 0.25, base))

