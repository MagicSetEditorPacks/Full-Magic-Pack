	#example: Thousand-legged Kami

keyword:
	#english keyword: Splice
	keyword: Imprégnation
	match: Imprégnation <atom-param>nom</atom-param> <atom-param>coût</atom-param>
	mode: expert
	reminder: Au moment où vous lancez un sort {param1}, vous pouvez révéler cette carte depuis votre main et payer son coût d'imprégnation. Si vous faites ainsi, ajoutez les effets de cette carte à ce sort.
	#example: Everdream

