		fallback          : "English"
		pt_separator      : "/"
		type_separator    : " : "
		subtype_separator : "<atom-sep> et </atom-sep>"
		keyword_types     : ["vieux","noyau","expert","pseudo","action","personnalisé"]

		is_artifact       : match@(match: "(?i)Artefact")
		is_battle         : match@(match: "(?i)Bataille")
		is_creature       : match@(match: "(?i)Créature")
		is_kindred        : match@(match: "(?i)(de clan|tribal)")
		is_tribal         : match@(match: "(?i)(de clan|tribal)")
		is_enchantment    : match@(match: "(?i)Enchantement")
		is_land           : match@(match: "(?i)Terrain")
		is_planeswalker   : match@(match: "(?i)Planeswalker")
		is_spell          : match@(match: "(?i)Éphémère|Rituel")
		is_instant        : match@(match: "(?i)Éphémère")
		is_sorcery        : match@(match: "(?i)Rituel")

		is_nonstandard    : match@(match: "(?i)(Conspiration|Donjon|Emblème|Héros|Phénomène|Plan|Schème|Jeton|Avant-garde)")
		is_conspiracy     : match@(match: "(?i)Conspiration")
		is_dungeon        : match@(match: "(?i)Donjon")
		is_emblem         : match@(match: "(?i)Emblème")
		is_hero           : match@(match: "(?i)Héros")
		is_token          : match@(match: "(?i)jeton")
		is_phenomenon     : match@(match: "(?i)Phénomène")
		is_plane		  : match@(match: "(?i)Plan\\b")
		is_scheme         : match@(match: "(?i)Schème")
		is_vanguard       : match@(match: "(?i)Avant-garde")

		is_basic          : match@(match: "(?i)de base")
		is_elite          : match@(match: "(?i)élite")
		is_host           : match@(match: "(?i)hôte")
		is_legendary      : match@(match: "(?i)légendaire")
		is_ongoing        : match@(match: "(?i)en cours")
		is_snow           : match@(match: "(?i)neigeux|neigeuse|enneigé|enneigée")
		is_world          : match@(match: "(?i)du monde")
		get_supertypes    : filter_text@(match: "(?i)(légendaire|de base|neigeux|neigeuse|enneigé|enneigée|Jeton|du monde|en cours|Élite|Hôte) ?")
		remove_supertypes : replace_text@(match: "(?i)(légendaire|de base|neigeux|neigeuse|enneigé|enneigée|Jeton|du monde|en cours|Élite|Hôte)[- ]?", replace:"")
		base_supertypes   : "légendaire|de base|neigeux|neigeuse|enneigé|enneigée|Jeton|du monde|en cours|Élite|Hôte"

